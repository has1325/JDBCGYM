package space.jdbc.basic;

public class Memo {
	
	private long id;
	private String content;
	private long a;
	
	public Memo() {}
	
	public Memo(long id, String content) {
		this.id = id;
		this.content = content;
	}
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	public long getA() {
		return a;
	}

	public void setA(long a) {
		this.a = a;
	}

	@Override
	public String toString() {
		return "Memo2 [id=" + id + ", content=" + content + ", a=" + a + ", getId()=" + getId() + ", getContent()="
				+ getContent() + ", getA()=" + getA() + "]";
	}

	
	
	
}
