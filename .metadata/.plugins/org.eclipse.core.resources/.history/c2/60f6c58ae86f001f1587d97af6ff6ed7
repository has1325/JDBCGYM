public class MainTest {

	public static void main(String[] args) {
		PrecureDao precureDao = new JdbcPrecureDao();
		
		Precure precure = new Precure();
		
		precure.setId(1);
		precure.setName("큐어 블랙");
		precure.setAge(15);
		precure.setColor("검정");
		precure.setType("격투");
		precure.setBirth(Date.valueOf("2025-10-10"));
		precure.setItem("카드 코뮨");
		precure.setComments("ありえない！");		
		
		precureDao.insert(precure);

	}

}
