package space.jdbc.basic;

public class Product {
	
	private long id;
	private String name;
	private long price;
	private String kind;
	
	public Product(){}
	
	public Product(long id, String name, long price, String kind) {
		this.id = id;
		this.kind = kind;
		this.name = name;
		this.price = price;
	}
}
