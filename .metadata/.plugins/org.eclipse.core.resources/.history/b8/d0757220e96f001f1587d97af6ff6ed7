package space.jdbc.precure;

import java.sql.Date;
import java.util.List;

public class MainTest {

	public static void main(String[] args) {
		PrecureDao precureDao = new JdbcPrecureDao();
		
		Precure precure = new Precure();
		
		precure.setId(2);
		precure.setName("큐어 화이트");
		precure.setAge(15);
		precure.setColor("하양");
		precure.setType("기술");
		precure.setBirth(Date.valueOf("2025-04-04"));
		precure.setItem("카드 코뮨");
		precure.setComments("光の使者");
		
		precureDao.insert(precure);

	}

}
