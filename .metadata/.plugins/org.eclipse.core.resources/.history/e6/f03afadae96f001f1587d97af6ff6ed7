package space.jdbc.precure;

import java.sql.Date;
import java.util.List;

public class MainTest {

	public static void main(String[] args) {
		PrecureDao precureDao = new JdbcPrecureDao();
		
		Precure precure = new Precure();
		
		precure.setId(5);
		precure.setName("큐어 스카이");
		precure.setAge(15);
		precure.setColor("파랑");
		precure.setType("하늘");
		precure.setBirth(Date.valueOf("2025-09-20"));
		precure.setItem("스카이 미라쥬");
		precure.setComments("ヒーローの出番です！");
		
		precureDao.insert(precure);

	}

}
