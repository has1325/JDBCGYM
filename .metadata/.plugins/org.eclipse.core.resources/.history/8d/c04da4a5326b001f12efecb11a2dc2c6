package memo1;

public class Member {
	private int id;
	private String content;
	
	Member(){}
	
	Member(int id, String content){
		this.id = id;
		this.content = content;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	@Override
	public String toString() {
		return "Member [id=" + id + ", content=" + content + ", getId()=" + getId() + ", getContent()=" + getContent()
				+ "]";
	}
	
	
}
